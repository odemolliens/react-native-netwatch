{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb-base",
    "tslint-react",
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-unused-declaration": {
      "options": [{
        "declarations": false,
        "ignored": {},
        "imports": true
      }],
      "severity": "error"
    },
    "object-literal-key-quotes": false,
    "no-console": false,
    "ban-types": false,
    "member-access": false,
    "array-type": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "import-spacing": true,
    "no-var-requires": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "jsx-no-lambda": false,
    "jsx-key": false,
    "jsx-boolean-value": [
      "never"
    ],
    "no-magic-numbers": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "ignore"
        },
        "singleline": "never",
        "esSpecCompliant": true
      }
    ],
    "no-empty": false,
    "arrow-parens": false,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "max-classes-per-file": false,
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "whitespace": [
      true
    ],
    "max-line-length": [
      true,
      140
    ]
  },
  "rulesDirectory": []
}
